<?xml version="1.0" encoding="utf-8"?>
<ip>

	<!-- Автор: Shtys -->

	<!-- Список возможных макросов:
	{{n}}					- переход на новую строку
	{{t}}					- фиксированный отступ (равнозначен TAB)
	{{target}} - название танка выбранной цели
	{{target-gun}} - названия орудия танка цели
	{{target-reload-100}} - примерное время перезарядки орудия танка цели с округлением до сотых (с досылателем и 110% экипажем)
	{{target-reload-10}} - примерное время перезарядки орудия танка цели с округлением до десятых (с досылателем и 110% экипажем)
	{{target-reload}} - примерное время перезарядки орудия танка цели с округлением до целых (с досылателем и 110% экипажем)
	{{vision}}				- радиус обзора танка цели (для 100% экипажа)
	{{player-weight}}		- масса своего танка в тоннах
	{{weight}}				- масса танка выбранной цели в тоннах
	{{sw}}					- символы сравнения массы своего танка с массой танка цели
	{{swi}}					- символы сравнения массы танка цели с массой своего танка 
	{{hull-front}} - лобовое бронирование корпуса танка цели
	{{hull-side}} - бортовое бронирование корпуса танка цели
	{{hull-back}} - бронирование кормы корпуса танка цели
	{{turret-front}} - лобовое бронирование башни танка цели
	{{turret-side}} - бортовое бронирование башни танка цели
	{{turret-back}} - бронирование кормы башни танка цели
	{{damage-1}} - урон первого типа снаряда танка цели
	{{damage-2}} - урон второго типа снаряда танка цели
	{{damage-3}} - урон третьего типа снаряда танка цели
	{{power-1}} - бронепробиваемость первого типа снаряда танка цели
	{{power-2}} - бронепробиваемость второго типа снаряда танка цели
	{{power-3}} - бронепробиваемость третьего типа снаряда танка цели
	{{type-1}} - первый тип снаряда танка цели
	{{type-2}} - второй тип снаряда танка цели
	{{type-3}} - третий тип снаряда танка цели -->	

	<!-- Глобальные настройки панели информации о цели -->
	<infoPanel>
		<name>
			<ru>цветной</ru>
			<en>colored</en>
		</name>
		
		<lng>
			<item macro="{{lng-sec}}" ru="сек" en="sec"/>
			<item macro="{{lng-vision}}" ru="Огляд" en="Vision"/>
			<item macro="{{lng-m}}" ru="м" en="m"/>
			<item macro="{{lng-turret}}" ru="Башта" en="Turret"/>
			<item macro="{{lng-hull}}" ru="Корпус:" en="Hull:{{t}}"/>
			<item macro="{{lng-type}}" ru="Тип" en="Type"/>
			<item macro="{{lng-damage}}" ru="Шкода:{{t}}" en="Damage:"/>
			<item macro="{{lng-piercing}}" ru="Пробиття" en="Piercing"/>
			<item macro="{{lng-weight}}" ru="Маса" en="Weight"/>
			<item macro="{{lng-t}}" ru="т" en="t"/>
		</lng>
		
		<!-- Аркадный прицел -->
		<arcade x="-250" y="250"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет от центра экрана, в аркадном прицеле перекрестие не является центром экрана -->
		
		<!-- Снайперский прицел -->
		<sniper x="-250" y="250"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет от центра экрана, в аркадном прицеле перекрестие не является центром экрана -->
		
		<!-- Артиллерийский прицел -->
		<strategic x="-250" y="250"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет от центра экрана, в аркадном прицеле перекрестие не является центром экрана -->
		
		<!-- Посмертный прицел -->
		<postmortem visible="true" x="-250" y="250"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет от центра экрана, в аркадном прицеле перекрестие не является центром экрана -->
		
	</infoPanel>
	
	<!-- Настройка фона панели -->
	<background color="0x000000" alpha="30" x="-85" y="-125" width="170" height="240"/>

	<!-- Настройка названия танка цели -->
	<targetName visible="true"> <!-- visible - видимость элемента, true/false - включено/выключено -->
		<location x="-80" y="-120"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет внутри самой панели в зависимости от ее положения и по умолчанию от низа и центра иконки танка -->
		<text font="Cuprum" size="15" color="0xFFFFFF" alpha="100" align="left" bold="true" leadintmlg="0" width="140" height="35"/> <!-- font - шрифт. size - размер шрифта. color - цвет шрифта. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). align - выравнивание текста, left/right/center - лево/право/центр. bold - жирный тест, true/false - включено/выключено. leading - межстрочный интервал. width, height - ширина и высота поля -->
		<filter size="3" angle="0" distance="0" color="0x000000" alpha="100" strength="200"/> <!-- size - размер тени. angle - угол тени. distance - расстояние смещения тени. color - цвет тени. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). strength - степень нанесения тени в процентах -->
		<format>{{target}}</format> <!-- Как будет выглядеть строка в игре. Формируется она из макросов и(или) любых символов. Список макросов перечислен в начале конфига -->
	</targetName>
	
	<!-- Настройка названия орудия танка цели -->
	<gunName visible="false"> <!-- visible - видимость элемента, true/false - включено/выключено -->
		<location x="-80" y="-100"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет внутри самой панели в зависимости от ее положения и по умолчанию от низа и центра иконки танка -->
		<text font="Cuprum" size="15" color="0xFFFFFF" alpha="100" align="left" bold="false" leading="0" width="300" height="35"/> <!-- font - шрифт. size - размер шрифта. color - цвет шрифта. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). align - выравнивание текста, left/right/center - лево/право/центр. bold - жирный тест, true/false - включено/выключено. leading - межстрочный интервал. width, height - ширина и высота поля -->
		<filter size="3" angle="0" distance="0" color="0x000000" alpha="100" strength="200"/> <!-- size - размер тени. angle - угол тени. distance - расстояние смещения тени. color - цвет тени. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). strength - степень нанесения тени в процентах -->
		<format>{{target-gun}}</format> <!-- Как будет выглядеть строка в игре. Формируется она из макросов и(или) любых символов. Список макросов перечислен в начале конфига -->
	</gunName>
	
	<!-- Настройка примерного времени перезарядки орудия танка цели (с досылателем и 110% экипажем) -->
	<gunReload visible="true"> <!-- visible - видимость элемента, true/false - включено/выключено -->
		<location x="-80" y="-100"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет внутри самой панели в зависимости от ее положения и по умолчанию от низа и центра иконки танка -->
		<text font="Cuprum" size="15" color="0xFF0000" alpha="100" align="left" bold="false" leading="0" width="160" height="35"/> <!-- font - шрифт. size - размер шрифта. color - цвет шрифта. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). align - выравнивание текста, left/right/center - лево/право/центр. bold - жирный тест, true/false - включено/выключено. leading - межстрочный интервал. width, height - ширина и высота поля -->
		<filter size="3" angle="0" distance="0" color="0x000000" alpha="100" strength="220"/> <!-- size - размер тени. angle - угол тени. distance - расстояние смещения тени. color - цвет тени. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). strength - степень нанесения тени в процентах -->
		<format>{{target-reload-10}} {{lng-sec}}</format> <!-- Как будет выглядеть строка в игре. Формируется она из макросов и(или) любых символов. Список макросов перечислен в начале конфига -->
	</gunReload>
	
	<!-- Настройка радиуса обзора танка цели -->
	<visionRadius visible="true"> <!-- visible - видимость элемента, true/false - включено/выключено -->
		<location x="-80" y="-80"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет внутри самой панели в зависимости от ее положения и по умолчанию от низа и центра иконки танка -->
		<text font="Cuprum" size="15" color="0x00BBFF" alpha="100" align="left" bold="false" leading="0" width="160" height="35"/> <!-- font - шрифт. size - размер шрифта. color - цвет шрифта. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). align - выравнивание текста, left/right/center - лево/право/центр. bold - жирный тест, true/false - включено/выключено. leading - межстрочный интервал. width, height - ширина и высота поля -->
		<filter size="3" angle="0" distance="0" color="0x000000" alpha="100" strength="250"/> <!-- size - размер тени. angle - угол тени. distance - расстояние смещения тени. color - цвет тени. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). strength - степень нанесения тени в процентах -->
		<format>{{vision}}{{lng-m}}</format> <!-- Как будет выглядеть строка в игре. Формируется она из макросов и(или) любых символов. Список макросов перечислен в начале конфига -->
	</visionRadius>
	
	<!-- Настройка лобового бронирования башни -->
	<turretArmorF visible="true"> <!-- visible - видимость элемента, true/false - включено/выключено -->
		<location x="-70" y="0"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет внутри самой панели в зависимости от ее положения и по умолчанию от низа и центра иконки танка -->
		<text font="Cuprum" size="15" color="0xFF8000" alpha="100" align="left" bold="false" leading="0" width="160" height="35"/> <!-- font - шрифт. size - размер шрифта. color - цвет шрифта. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). align - выравнивание текста, left/right/center - лево/право/центр. bold - жирный тест, true/false - включено/выключено. leading - межстрочный интервал. width, height - ширина и высота поля -->
		<filter size="3" angle="0" distance="0" color="0x000000" alpha="100" strength="200"/> <!-- size - размер тени. angle - угол тени. distance - расстояние смещения тени. color - цвет тени. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). strength - степень нанесения тени в процентах -->
		<format>†{{t}}{{turret-front}} / {{turret-side}} / {{turret-back}}</format> <!-- Как будет выглядеть строка в игре. Формируется она из макросов и(или) любых символов. Список макросов перечислен в начале конфига -->
	</turretArmorF>
	
	<!-- Настройка бортового бронирования башни -->
	<turretArmorS visible="false"> <!-- visible - видимость элемента, true/false - включено/выключено -->
		<location x="26" y="18"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет внутри самой панели в зависимости от ее положения и по умолчанию от низа и центра иконки танка -->
		<text font="Cuprum" size="15" color="0x00FF00" alpha="100" align="left" bold="false" leading="0" width="160" height="35"/> <!-- font - шрифт. size - размер шрифта. color - цвет шрифта. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). align - выравнивание текста, left/right/center - лево/право/центр. bold - жирный тест, true/false - включено/выключено. leading - межстрочный интервал. width, height - ширина и высота поля -->
		<filter size="3" angle="0" distance="0" color="0x000000" alpha="100" strength="200"/> <!-- size - размер тени. angle - угол тени. distance - расстояние смещения тени. color - цвет тени. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). strength - степень нанесения тени в процентах -->
		<format>{{turret-side}}</format> <!-- Как будет выглядеть строка в игре. Формируется она из макросов и(или) любых символов. Список макросов перечислен в начале конфига -->
	</turretArmorS>
	
	<!-- Настройка бронирования кормы башни -->
	<turretArmorB visible="false"> <!-- visible - видимость элемента, true/false - включено/выключено -->
		<location x="60" y="18"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет внутри самой панели в зависимости от ее положения и по умолчанию от низа и центра иконки танка -->
		<text font="Cuprum" size="15" color="0x00FF00" alpha="100" align="left" bold="false" leading="0" width="160" height="35"/> <!-- font - шрифт. size - размер шрифта. color - цвет шрифта. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). align - выравнивание текста, left/right/center - лево/право/центр. bold - жирный тест, true/false - включено/выключено. leading - межстрочный интервал. width, height - ширина и высота поля -->
		<filter size="3" angle="0" distance="0" color="0x000000" alpha="100" strength="200"/> <!-- size - размер тени. angle - угол тени. distance - расстояние смещения тени. color - цвет тени. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). strength - степень нанесения тени в процентах -->
		<format>{{turret-back}}</format> <!-- Как будет выглядеть строка в игре. Формируется она из макросов и(или) любых символов. Список макросов перечислен в начале конфига -->
	</turretArmorB>
	
	<!-- Настройка лобового бронирования корпуса -->
	<hullArmorF visible="true"> <!-- visible - видимость элемента, true/false - включено/выключено -->
		<location x="-70" y="18"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет внутри самой панели в зависимости от ее положения и по умолчанию от низа и центра иконки танка -->
		<text font="Cuprum" size="15" color="0xFF8000" alpha="100" align="left" bold="false" leading="0" width="160" height="35"/> <!-- font - шрифт. size - размер шрифта. color - цвет шрифта. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). align - выравнивание текста, left/right/center - лево/право/центр. bold - жирный тест, true/false - включено/выключено. leading - межстрочный интервал. width, height - ширина и высота поля -->
		<filter size="3" angle="0" distance="0" color="0x000000" alpha="100" strength="200"/> <!-- size - размер тени. angle - угол тени. distance - расстояние смещения тени. color - цвет тени. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). strength - степень нанесения тени в процентах -->
		<format>‡{{t}}{{hull-front}} / {{hull-side}} / {{hull-back}}</format> <!-- Как будет выглядеть строка в игре. Формируется она из макросов и(или) любых символов. Список макросов перечислен в начале конфига -->
	</hullArmorF>
	
	<!-- Настройка бортового бронирования корпуса -->
	<hullArmorS visible="false"> <!-- visible - видимость элемента, true/false - включено/выключено -->
		<location x="27" y="36"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет внутри самой панели в зависимости от ее положения и по умолчанию от низа и центра иконки танка -->
		<text font="Cuprum" size="15" color="0xFFFF00" alpha="100" align="left" bold="false" leading="0" width="160" height="35"/> <!-- font - шрифт. size - размер шрифта. color - цвет шрифта. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). align - выравнивание текста, left/right/center - лево/право/центр. bold - жирный тест, true/false - включено/выключено. leading - межстрочный интервал. width, height - ширина и высота поля -->
		<filter size="3" angle="0" distance="0" color="0x000000" alpha="100" strength="200"/> <!-- size - размер тени. angle - угол тени. distance - расстояние смещения тени. color - цвет тени. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). strength - степень нанесения тени в процентах -->
		<format>{{hull-side}}</format> <!-- Как будет выглядеть строка в игре. Формируется она из макросов и(или) любых символов. Список макросов перечислен в начале конфига -->
	</hullArmorS>
	
	<!-- Настройка бронирования кормы корпуса -->
	<hullArmorB visible="false"> <!-- visible - видимость элемента, true/false - включено/выключено -->
		<location x="59" y="36"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет внутри самой панели в зависимости от ее положения и по умолчанию от низа и центра иконки танка -->
		<text font="Cuprum" size="15" color="0xFFFF00" alpha="100" align="left" bold="false" leading="0" width="160" height="35"/> <!-- font - шрифт. size - размер шрифта. color - цвет шрифта. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). align - выравнивание текста, left/right/center - лево/право/центр. bold - жирный тест, true/false - включено/выключено. leading - межстрочный интервал. width, height - ширина и высота поля -->
		<filter size="3" angle="0" distance="0" color="0x000000" alpha="100" strength="200"/> <!-- size - размер тени. angle - угол тени. distance - расстояние смещения тени. color - цвет тени. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). strength - степень нанесения тени в процентах -->
		<format>{{hull-back}}</format> <!-- Как будет выглядеть строка в игре. Формируется она из макросов и(или) любых символов. Список макросов перечислен в начале конфига -->
	</hullArmorB>
	
	<!-- Настройка типа первого снаряда -->
	<shellType1 visible="true"> <!-- visible - видимость элемента, true/false - включено/выключено -->
		<location x="-70" y="60"/> <!-- x, y - координаты расположения элемента. Отсчет координат идет внутри самой панели в зависимости от ее положения и по умолчанию от низа и центра иконки танка -->
		<text font="Cuprum" size="15" color="0xFFFF00" alpha="100" align="left" bold="false" leading="0" width="500" height="35"/> <!-- font - шрифт. size - размер шрифта. color - цвет шрифта. alpha - прозрачность тени(100 - непрозрачно, 0 - прозрачно). align - выравнивание текста, left/right/center - лево/право/центр. bold - жирный тест, true/false - включено/выключено. leading - межстрочный интервал. width, height - ширина и высота поля -->
		<filter size="3" angle="0" distance="0" color="0x000000" alpha="100" strength="350"/> <!-- size - the size of the shadow. angle-the angle of the shadow. distance-distance the shadow offsets. color-the color of the shadow. alpha-the transparency of the shadow (100-0, opaque-transparent). strength-the degree of shadow as a percentage -->
		<format>№{{t}}{{type-1}} / {{type-2}} / {{type-3}}</format> <!-- Как будет выглядеть строка в игре. Формируется она из макросов и(или) любых символов. Список макросов перечислен в начале конфига -->
	</shellType1>
	
	<!-- Настройка типа второго снаряда -->
